<section class="jumbotron">

          <div class="container">
<h3 class="jumbotron-heading">Create New Item</h3>
<form method="POST" action="/items">
  <label for="item_name">Item Name:</label>
<input type="text" name="item[name]" id="item_name"><br><br>
<label>What list does this item belong to? </label>
<%List.all.each do |list|%><br>
<input type="checkbox" name="item[list_id]" id="<%=list.name %>"
  value="<%=list.id%>"> <%=list.name%></input>
 <%end%> <br></br>
   <input type="submit"  class="btn btn-primary" value="Create Item">
</form>
</div></section>
